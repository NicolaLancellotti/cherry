// ___________________________________________________________________________
// Parse Declarations

ERROR(expected_fun, "expected `fun`")

// prototype
ERROR(expected_id,
    "expected identifier")
ERROR(expected_colon,
     "expected `:`")
ERROR(expected_type,
     "expected type")
ERROR(expected_l_paren_in_arg_list,
    "expected `(` in argument list of function declaration")

// ___________________________________________________________________________
// Parse Expressions

ERROR(expected_expr,
    "expected expression")
ERROR(expected_decimal_or_id,
    "expected decimal or identifier")

ERROR(integer_literal_overflows,
    "integer literal overflows")

// function body
ERROR(expected_l_brace_func_body,
    "expected `{` in body of function declaration")
ERROR(expected_r_brace_func_body,
    "expected `}` at end of brace statement")
ERROR(expected_semi,
    "expected `;` at end of declaration")

// function call
ERROR(expected_l_paren_func_call,
    "expected `(` to begin a function call")
ERROR(expected_comma_or_r_paren_arg_list,
    "expected `,` or `)` in argument list")

// ___________________________________________________________________________
#undef ERROR
